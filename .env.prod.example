APP_ENV=prod

# Database (RDS)
DATABASE_URL=postgresql://user:password@your-rds-host.rds.amazonaws.com/crimata

# Auth
SECRET_KEY=<long-random-string>
PASSPHRASE=<your-passphrase>

# Stripe (live keys â€” https://dashboard.stripe.com/apikeys)
STRIPE_SECRET_KEY=sk_live_...
STRIPE_PUBLISHABLE_KEY=pk_live_...
STRIPE_PRICE_ID=price_...
STRIPE_WEBHOOK_SECRET=whsec_...
BASE_URL=https://crimata.com

# AWS
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=...
AWS_SECRET_ACCESS_KEY=...

# S3 (bucket must have public read access for images)
S3_BUCKET=crimata-uploads

# Shared infrastructure (provisioned via Terraform)
ECS_CLUSTER=crimata-cluster
ECR_IMAGE=123456789.dkr.ecr.us-east-1.amazonaws.com/crimata:latest
VPC_ID=vpc-...
VPC_SUBNETS=subnet-...,subnet-...
SECURITY_GROUP=sg-...
ALB_LISTENER_ARN=arn:aws:elasticloadbalancing:...
ALB_DNS_NAME=crimata-alb-xxx.us-east-1.elb.amazonaws.com
ALB_ZONE_ID=Z35SXDOTRQ7X7K
TASK_EXECUTION_ROLE_ARN=arn:aws:iam::123456789:role/crimata-task-execution

# RDS (shared instance for per-customer databases)
RDS_HOST=crimata.xxx.us-east-1.rds.amazonaws.com
RDS_PORT=5432
RDS_MASTER_USER=crimata_master
RDS_MASTER_PASSWORD=...

# Route53
HOSTED_ZONE_ID=Z...

# SES
SES_FROM_EMAIL=noreply@crimata.com
